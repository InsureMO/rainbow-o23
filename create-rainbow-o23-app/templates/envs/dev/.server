# pipeline logger, if enable pipeline debug log, need to enable one of error/combined/console log.
CFG_PIPELINE_DEBUG_LOG_ENABLED=false
CFG_PIPELINE_PERFORMANCE_LOG_ENABLED=false
# enable example controllers
CFG_APP_EXAMPLES_ENABLED=true
CFG_APP_API_TEST=true
# pipeline directory
CFG_APP_INIT_PIPELINES_DIR=server
# plugins, all plugins default disabled
CFG_APP_PLUGINS_PRINT=true
CFG_APP_PLUGINS_AWS_S3=true
# excluded pipeline directory
#CFG_APP_EXCLUDED_PIPELINES_DIRS=some-directory
# for homepage proxy
CFG_ENDPOINTS_BAIDU_GLOBAL_HEADERS=
CFG_ENDPOINTS_BAIDU_GLOBAL_TIMEOUT=60
CFG_ENDPOINTS_BAIDU_HOME_URL=https://baidu.com
CFG_ENDPOINTS_BAIDU_HOME_HEADERS=
CFG_ENDPOINTS_BAIDU_HOME_TIMEOUT=30
